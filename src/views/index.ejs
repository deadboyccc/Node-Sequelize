<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users List</title>
</head>

<body>
  <h1>Users List</h1>
  <ol>
    <% users.forEach(user=> { %>
      <li>
        <%= user.name %> - <%= user.email %>
            <% if (user.posts.length> 0) { %>
              <ul>
                <% user.posts.forEach(post=> { %>
                  <li>
                    <%= post.text %> - <%= post.votes %>
                  </li>
                  <% }) %>
              </ul>
              <% } %>
      </li>
      <% }) %>
  </ol>
</body>

</html>